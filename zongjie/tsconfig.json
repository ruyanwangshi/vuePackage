{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "ES2016", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": false
  }
}
